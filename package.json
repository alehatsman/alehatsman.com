{
  "name": "alehatsman.com",
  "version": "1.0.0",
  "private": true,
  "description": "Software Engineer Blog",
  "author": "Aleh Atsman <aleh.atsman@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atsman/alehatsman.com.git"
  },
  "bugs": {
    "url": "https://github.com/atsman/alehatsman.com/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:update-snapshots": "jest -u",
    "test:watch": "jest --watch",
    "test:watch:coverage": "jest --watchAll --coverage",
    "types:check": "tsc --noEmit",
    "check": "yarn run lint:fix && yarn run types:check && yarn run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check"
    }
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.3.1",
    "gatsby-image": "^3.3.0",
    "gatsby-plugin-disqus": "^1.2.2",
    "gatsby-plugin-emotion": "^6.3.0",
    "gatsby-plugin-feed": "^3.3.0",
    "gatsby-plugin-google-analytics": "^3.3.0",
    "gatsby-plugin-image": "^1.3.1",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-mdx": "^2.3.0",
    "gatsby-plugin-module-resolver": "^1.0.3",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-typescript": "^3.3.0",
    "gatsby-remark-copy-linked-files": "^4.0.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.0.0",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-source-sanity": "^7.0.4",
    "gatsby-transformer-sharp": "^3.3.0",
    "prism-react-renderer": "^1.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.2.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/jest": "^11.3.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-system": "^5.1.11",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-gatsby": "^1.4.0",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "typescript": "^4.2.4",
    "typescript-language-server": "^0.5.1"
  }
}
